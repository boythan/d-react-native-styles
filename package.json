{
    "name": "d-react-native-styles",
    "version": "0.0.16",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "build": "npm run build:types && npm run build:js",
        "build:types": "tsc --p tsconfig.build.json",
        "build:js": "babel src --out-dir dist --config-file ./.babelrc.json --extensions \".ts,.tsx\" --ignore 'src/__test__/*' --source-maps inline",
        "build:lib": "./node_modules/.bin/babel src --out-dir dist --config-file ./.babelrc.json --extensions '.ts,.tsx' --ignore 'src/__test__/*'",
        "publishD": "yarn build && yarn publish"
    },
    "dependencies": {
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.12.9",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.10.1",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.182",
        "@types/react-native": "^0.67.3",
        "@types/react-test-renderer": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-typescript-to-proptypes": "^1.3.2",
        "babel-preset-react-native": "*",
        "eslint": "^7.32.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-native": "^4.0.0",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "^0.67.0",
        "react": "17.0.2",
        "react-native": "0.68.2",
        "react-test-renderer": "17.0.2",
        "typescript": "^4.4.4"
    },
    "resolutions": {
        "@types/react": "^17"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
